FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
libs
(
 "libHybridTurbulenceModel.so"
);

application     pisoFoam;

startFrom       startTime;

startTime       0;

stopAt          writeNow;//endTime;

endTime         0.8;

deltaT          0.00004;

writeControl    timeStep;

writeInterval   5000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep false;

maxCo          0.2;

maxDeltaT      0.002;

functions
{
    #include "forces"
}
